{
  "$schema": "https://vega.github.io/schema/vega/v3.0.json",
  "autosize": "pad",
  "padding": 5,
  "width": 480,
  "height": 280,
  "title": {
    "text": %1%,
    "offset": 90
  },
  "style": "cell",
  "data": [
    {
      "name": "source_2",
      "values": [
        {
          "x": 100000000,
          "y": "loading_data_from_tc",
          "count": 0
        }
      ]
    }
  ],
  "marks": [
    {
      "encode": {
        "enter": {
          "x": {
            "value": 0
          },
          "y": {
            "value": 0
          },
          "height": {
            "value": 280
          },
          "clip": {
            "value": 0
          },
          "fill": {
            "value": "#ffffff"
          },
          "fillOpacity": {
            "value": 0
          }
        }
      },
      "marks": [
        {
          "name": "marks",
          "type": "rect",
          "style": [
            "rect"
          ],
          "from": {
            "data": "source_2"
          },
          "encode": {
            "update": {
              "x": {
                "scale": "x",
                "field": "x"
              },
              "width": {
                "scale": "x",
                "band": true
              },
              "y": {
                "scale": "y",
                "field": "y"
              },
              "height": {
                "scale": "y",
                "band": true
              },
              "fill": {
                "scale": "color",
                "field": "count"
              }
            }
          }
        }
      ],
      "type": "group",
      "scales": [
        {
          "name": "x",
          "type": "band",
          "domain": {
            "data": "source_2",
            "field": "x",
            "sort": true
          },
          "range": [
            0,
            480
          ],
          "paddingInner": 0.1,
          "paddingOuter": 0.05
        },
        {
          "name": "y",
          "type": "band",
          "domain": {
            "data": "source_2",
            "field": "y",
            "sort": true
          },
          "range": [
            280,
            0
          ],
          "paddingInner": 0.1,
          "paddingOuter": 0.05
        },
        {
          "name": "color",
          "type": "sequential",
          "domain": {
            "data": "source_2",
            "field": "count"
          },
          "range": "heatmap",
          "nice": false,
          "zero": false
        }
      ],
      "axes": [
        {
          "scale": "x",
          "labelOverlap": true,
          "orient": "bottom",
          "title": %2%,
          "zindex": 1,
          "encode": {
            "labels": {
              "update": {
                "angle": {
                  "value": 270
                },
                "align": {
                  "value": "right"
                },
                "baseline": {
                  "value": "middle"
                }
              }
            }
          }
        },
        {
          "scale": "y",
          "orient": "left",
          "title": %3%,
          "zindex": 1
        }
      ],
      "legends": [
        {
          "orient": "top",
          "fill": "color",
          "title": "Count",
          "type": "gradient"
        }
      ]
    }
  ],
  "config": {
    "legend": {
      "titleFont": "HelveticaNeue, Arial",
      "titleColor": "rgba(0,0,0,0.65)",
      "labelFont": "HelveticaNeue",
      "labelColor": "rgba(0,0,0,0.65)",
      "cornerRadius": 30,
      "gradientWidth": 480
    },
    "axis": {
      "labelFont": "HelveticaNeue, Arial",
      "labelFontSize": 12,
      "labelPadding": 10,
      "labelColor": "rgba(0,0,0,0.65)",
      "titleFont": "HelveticaNeue-Medium, Arial",
      "titleFontWeight": "normal",
      "titlePadding": 30,
      "titleFontSize": 15,
      "titleColor": "rgba(0,0,0,0.65)"
    },
    "axisY": {
      "minExtent": 30
    },
    "style": {
      "group-title": {
        "fontSize": 29,
        "font": "HelveticaNeue, Arial",
        "fontWeight": "normal",
        "fill": "rgba(0,0,0,0.65)"
      }
    },
    "range": {
      "heatmap": {
        "scheme": "greenblue"
      }
    }
  }
}
